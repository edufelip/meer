{
  "expo": {
    "name": "Guia do Brechó",
    "slug": "meer",
    "owner": "eduardo880",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "meer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "appConfig": "./app.config.js",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edufelip.meer",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.edufelip.meer",
      "googleServicesFile": "./config/google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que o Guia Brechó acesse sua localização para mostrar brechós perto de você."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "ac66eaa0-2006-4371-8b97-10ce159f01d2"
      }
    }
  }
}
