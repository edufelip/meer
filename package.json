{
  "name": "meer",
  "main": "expo/AppEntry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "mock:server": "node mock-server/server.js",
    "e2e:build:ios": "detox build -c ios.sim.debug",
    "e2e:build:android": "detox build -c android.emu.debug",
    "e2e:ios": "detox test -c ios.sim.debug",
    "e2e:android": "detox test -c android.emu.debug",
    "e2e:build:ios:mock": "node scripts/with-api-base-url.js --base-url http://localhost:4010 -- npm run e2e:build:ios",
    "e2e:build:android:mock": "node scripts/with-api-base-url.js --base-url http://localhost:4010 -- npm run e2e:build:android",
    "e2e:ios:mock": "node scripts/with-api-base-url.js --base-url http://localhost:4010 -- npm run e2e:ios",
    "e2e:android:mock": "node scripts/with-api-base-url.js --base-url http://localhost:4010 -- npm run e2e:android",
    "e2e:ios:mock:all": "node scripts/run-e2e-mock.js --platform ios",
    "e2e:android:mock:all": "node scripts/run-e2e-mock.js --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/crashlytics": "^23.7.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-apple-authentication": "^8.0.7",
    "expo-auth-session": "^7.0.9",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "^19.0.7",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.0",
    "@types/react": "~19.1.0",
    "@types/jest": "^30.0.0",
    "detox": "^20.16.0",
    "express": "^4.21.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^30.0.5",
    "jest-expo": "~54.0.4",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2"
  },
  "detox": {
    "testRunner": {
      "args": {
        "$0": "jest",
        "config": "e2e/jest.config.js"
      }
    },
    "apps": {
      "ios.sim.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/GuiaBrecho.app",
        "build": "xcodebuild -workspace ios/Meer.xcworkspace -scheme Meer -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build"
      },
      "android.emu.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "testBinaryPath": "android/app/build/outputs/apk/androidTest/debug/app-debug-androidTest.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug"
      }
    },
    "devices": {
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 17"
        }
      },
      "emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_5_API_33"
        }
      }
    },
    "configurations": {
      "ios.sim.debug": {
        "device": "simulator",
        "app": "ios.sim.debug"
      },
      "android.emu.debug": {
        "device": "emulator",
        "app": "android.emu.debug"
      }
    }
  },
  "private": true
}
